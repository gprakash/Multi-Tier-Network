LOAD CSV with headers FROM 'https://raw.githubusercontent.com/gprakash/data/main/location.csv' AS line
CREATE (:Location {locationKey:line.locationId + ':' + line.locationType, locationId: line.locationId, locationType: line.locationType, locationDescription: line.locationDescription, locationLatitude:line.locationLatitutde, locationLongitude: line.locationLongitude, address: line.Address})
